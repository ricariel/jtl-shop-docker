<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Template isFullResponsive="true">
    <Name>CasaDuepur</Name>
    <Author>Fabrice Kirchner</Author>
    <URL>https://www.zyria.de</URL>
    <Parent>Evo</Parent>
    <Preview>preview.png</Preview>
    <Description>Child Template von Evo f√ºr Casa Due pur Hauptshop</Description>
    <Settings>
        <Section Name="Theme" Key="theme">
            <Setting Description="Theme" Key="theme_default" Type="select" Value="my-evo" override="true">
                <Option Value="my-evo">My Evo - mein eigenes Evo-Theme!</Option>
            </Setting>
        </Section>
    </Settings>
    <Minify>
        <CSS Name="my-evo.css">
            <File Path="./themes/my-evo/bootstrap.css"/>
            <File Path="../Evo/themes/base/offcanvas-menu.css"/>
            <File Path="../Evo/themes/base/pnotify.custom.css"/>
            <File Path="../Evo/themes/base/jquery-slider.css"/>
            <File Path="css/custom.css"/>
        </CSS>
				<JS Name="jtl3.js">
            <File Path="js/jquery.socialshareprivacy.min.js"/>
            <File Path="js/custom.js"/>
        </JS>	
    </Minify>
    <Boxes>
        <Container Position="right" Available="0"></Container>
    </Boxes>
</Template>
